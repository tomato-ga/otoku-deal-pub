{
    "regions": ["hnd1"],
      "git": {
        "deploymentEnabled": {
          "main": true
        }
      },
      "headers": [
        {
          "source": "/api/(.*)",
          "headers": [
            { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
          ]
        }
      ]
  }